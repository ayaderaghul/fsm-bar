VertexCircle[{xc_, yc_}, name_, {w_, h_}] := Disk[{xc, yc}, .1];
a0Graph =
   Graph[{-1 -> 1 ,
Labeled[0 -> 2, "L"] 
, Labeled[0 -> 1, "M"] 
, Labeled[0 -> 0, "H"] 
, Labeled[1 -> 2, "L"] 
, Labeled[1 -> 1, "M"] 
, Labeled[1 -> 0, "H"] 
, Labeled[2 -> 2, "L"] 
, Labeled[2 -> 1, "M"] 
, Labeled[2 -> 0, "H"] 
     },
   EdgeShapeFunction -> 
    GraphElementData["EdgeShapeFunction", "FilledArrow"],
   VertexStyle -> LightGray,
   VertexShapeFunction -> VertexCircle,
   VertexLabels -> {0 -> Placed["L", Center], 1 -> Placed["M", Center], 2 -> Placed["H", Center]}
   ];
G = Graphics[{White, Disk[{0, 0}, 0.2]}];
Show[a0Graph, G]
(*Export["a0.png",S]*)
 
